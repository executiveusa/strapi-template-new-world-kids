rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
    // virus-scan hook TODO comment.
    // TODO: Add virus scanning
    match /uploads/{userId}/{fileName} {
      allow write: if request.auth.uid == userId;
    }
  }
}